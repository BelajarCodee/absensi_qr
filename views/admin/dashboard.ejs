<!-- admin/dashboard.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Admin Dashboard</title>
</head>
<body>
  <h1>Admin Dashboard</h1>

  <table>
    <thead>
      <tr>
        <th>No</th>
        <th>Name</th>
        <th>Jurusan</th>
        <th>Kelasan</th>
        <th>Hari</th>
        <th>Masuk</th>
        <th>Pulang</th>
        <th>Keterangan</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach(function(item, index) { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= item.name %></td>
          <td><%= item.jurusan %></td>
          <td><%= item.kelasan %></td>
          <td><%= item.hari %></td>
          <td><%= item.masuk %></td>
          <td><%= item.pulang %></td>
          <td><%= item.keterangan %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>

</body>
</html>
